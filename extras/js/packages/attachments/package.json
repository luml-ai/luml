  },
  "peerDependencies": {
    "@primevue/themes": "^4.3.9",
    "dompurify": "^3.2.7",
    "lucide-vue-next": "^0.460.0",
    "marked": "^16.2.0",
    "pinia": "^2.2.6",
    "primevue": "^4.4.1",
