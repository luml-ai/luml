    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
